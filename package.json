{
  "private": true,
  "name": "smoking-heaps",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "build-storybook": "yarn fixtures && build-storybook",
    "clean": "rimraf storybook-static out .next",
    "data": "yarn -s data:fixtures && yarn -s data:csv && yarn -s data:google && yarn -s data:pdf",
    "data:csv": "yarn -s indexctl csv",
    "data:fixtures": "yarn -s indexctl fixtures",
    "data:google": "yarn -s indexctl google",
    "data:pdf": "yarn -s indexctl pdf",
    "deploy": "./.scripts/deploy.sh",
    "dev": "yarn clean && next dev",
    "export": "next export",
    "fix": "yarn lint --fix",
    "fixtures": "yarn data:fixtures",
    "indexctl": "ts-node --transpile-only -P tsconfig.ts-node.json bin/indexctl.ts",
    "lint": "eslint . --cache --ext ts --ext tsx --ext js --ext jsx",
    "navigation": "yarn indexctl navigation --write",
    "prod": "yarn build && yarn export",
    "start": "next start",
    "storybook": "yarn data && yarn fixtures && start-storybook -p 6006",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -w",
    "verify": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@sindresorhus/slugify": "^1.1.0",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "^1.1.1",
    "csv-parse": "^4.12.0",
    "d3": "^6.3.1",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-shape": "^2.0.0",
    "google-auth-library": "^6.1.3",
    "googleapis": "64",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "next": "10.0.5",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^2.1.3",
    "next-optimized-images": "^2.6.2",
    "next-svgr": "^0.0.2",
    "pretty": "^2.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intersection-observer": "^8.31.0",
    "react-select": "^3.2.0",
    "react-use": "^15.3.8",
    "scrollama": "^2.2.1",
    "swiper": "^6.4.5",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@svgr/webpack": "^5.5.0",
    "@types/cheerio": "^0.22.22",
    "@types/csv-parse": "^1.2.2",
    "@types/d3": "^5.16.3",
    "@types/d3-shape": "^2.0.0",
    "@types/node": "^14.11.9",
    "@types/pretty": "^2.0.0",
    "@types/puppeteer": "^5.4.2",
    "@types/react": "^16.9.52",
    "@types/react-select": "^3.0.27",
    "@types/swiper": "^5.4.1",
    "@types/unzipper": "^0.10.3",
    "@types/yargs": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "autoprefixer": "^10.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unicorn": "^22.0.0",
    "husky": "^4.3.0",
    "jimp": "^0.16.1",
    "lint-staged": "^10.4.1",
    "postcss": "^8.2.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "14.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "prettier-package-json": "^2.1.3",
    "puppeteer": "^5.5.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "responsive-loader": "^2.3.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.27.1",
    "tailwindcss": "^2.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "typescript-language-server": "^0.4.0",
    "webp-loader": "^0.6.0",
    "yargs": "^16.1.1"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "! git rev-parse --abbrev-ref HEAD | grep -q main || (yarn verify && lint-staged)"
    }
  },
  "lint-staged": {
    "*.{json,rc}": [
      "prettier --parser json --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ],
    "*.{js,ts,tsx}": [
      "yarn fix"
    ]
  }
}
